#include<iostream>

using namespace std;
int main()
{
cout<< "Content-type: text/html\n\n"<<endl;
cout<< "<html><body bgcolor=\"gray\">"<<endl;


cout<< "<h1>baby c++ chal pda </h1>"<<endl;

string a[2][2]={{"94011","2"},{"4011","1"}};

cout<<" <table border=1 style=\"width:100%\" >"<<endl ;
cout<<" <tr>"<<endl;
cout<<" <th>PLU CODE</th> "<<endl;
cout<<" <th> Weight </th> "<<endl;
cout<<" </tr> "<<endl;

for(int i=0 ; i<2 ; i++)
{
cout<<"<tr>"<<endl; 
	for(int j=0 ;j<2 ; j++)
	{ 
		cout<<"<td>"<<a[i][j]<<"</td>"<<endl; 
	}

cout<<" </tr>"<<endl;
}
cout<< "</table>"<<endl;

cout<< "</body></html>";


return 0;
}

